## Dynamic configuration

tls:
  stores:
    default:
      defaultCertificate:
        certFile: /etc/ssl/traefik/cf_cert.pem
        keyFile: /etc/ssl/traefik/cf_key.pem
  certificates:
    - certFile: /etc/ssl/traefik/cf_cert.pem
      keyFile: /etc/ssl/traefik/cf_key.pem

http:
  middlewares:
    crowdsec:
      plugin:
        crowdsec-bouncer-traefik-plugin:
          crowdsecLapiKey: {{ traefik_bouncer_key }}
          enabled: false
